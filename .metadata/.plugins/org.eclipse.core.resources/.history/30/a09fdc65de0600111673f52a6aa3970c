package recursividad;

import java.util.Iterator;

public class Recursividad {

	public static void main(String[] args) {
		
		int [] arreglo = {1,2,3,4,5};
		int mayor = obtenerMayorIterativo(arreglo);
		System.out.println(mayor);
		
		int mayor2 = obtenerMayorRecursivo(arreglo);
		
		
	}

	public static int obtenerMayorRecursivo(int [] arreglo) {
		return obtenerMayorRecursivo(arreglo,0,0);
	}

	private static int obtenerMayorRecursivo(int[] arreglo, int i, int j) {
		
		return 0;
	}
	
	
	
	

	private static int obtenerMayorIterativo(int[] arreglo) {
		int mayor = Integer.MIN_VALUE;
		for (int i = 0; i < arreglo.length; i++) {
			if(arreglo[i] > mayor) {
				mayor = arreglo[i];
			}
		}
		return mayor;
	}

}
